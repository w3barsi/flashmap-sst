#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules/sst/cli/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules/sst/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules/sst/cli/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules/sst/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/sst@2.39.2/node_modules:/Users/barsi/Dev/flashmap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/sst@2.39.2/node_modules/sst/cli/sst.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/sst@2.39.2/node_modules/sst/cli/sst.js" "$@"
fi
